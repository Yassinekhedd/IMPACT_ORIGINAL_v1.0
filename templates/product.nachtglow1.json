opIn {\n            0% {\n                transform: scale(0);\n                opacity: 0;\n            }\n            50% {\n                transform: scale(1.1);\n            }\n            100% {\n                transform: scale(1);\n                opacity: 1;\n            }\n        }\n        .badge-text {\n            display: flex;\n            flex-direction: column;\n            gap: 2px;\n        }\n        .badge-label {\n            font-size: 12px;\n            color: #6b7280;\n            font-weight: 400;\n        }\n        .badge-value {\n            font-size: 14px;\n            font-weight: 600;\n            color: #1f2937;\n        }\n        .arrow-divider {\n            color: #d1d5db;\n            font-size: 24px;\n            margin: 0 8px;\n            animation: fadeIn 0.4s ease-out 0.2s both;\n        }\n        @keyframes fadeIn {\n            from {\n                opacity: 0;\n            }\n            to {\n                opacity: 1;\n            }\n        }\n    </style>\n</head>\n<body>\n    <div class=\"badge-wrapper\">\n        <div class=\"shipping-badge\">\n            <div class=\"badge-section\">\n                <div class=\"icon-circle order-icon\">📦</div>\n                <div class=\"badge-text\">\n                    <span class=\"badge-label\">Bestellung</span>\n                    <span class=\"badge-value\">Heute</span>\n                </div>\n            </div>\n            <div class=\"arrow-divider\">→</div>\n            <div class=\"badge-section\">\n                <div class=\"icon-circle delivery-icon\">🇩🇪</div>\n                <div class=\"badge-text\">\n                    <span class=\"badge-label\">Versand in</span>\n                    <span class=\"badge-value\">48 Stunden</span>\n                </div>\n            </div>\n        </div>\n    </div>\n</body>\n</html>"
