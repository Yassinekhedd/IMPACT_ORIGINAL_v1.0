<option value=\"Blanc\">Weiß</option>\n        </select>\n      </div>\n\n      <div class=\"gift-selector\">\n        <label class=\"selector-label\">Größe wählen</label>\n        <select class=\"gift-select selector-select\" id=\"giftSizeSelect\">\n          <option value=\"\">Wählen Sie eine Größe</option>\n          <option value=\"XS\" disabled>XS - Ausverkauft</option>\n          <option value=\"S\">S</option>\n          <option value=\"M\">M</option>\n          <option value=\"L\">L</option>\n          <option value=\"XL\">XL</option>\n          <option value=\"2XL\" disabled>2XL - Ausverkauft</option>\n        </select>\n      </div>\n\n      <div class=\"gift-toggle-wrapper\">\n        <span class=\"gift-toggle-label\">Hinzufügen</span>\n        <div class=\"gift-toggle\" id=\"giftToggle\" onclick=\"toggleGift()\">\n          <div class=\"gift-toggle-slider\"></div>\n        </div>\n      </div>\n    </div>\n\n    <div class=\"gift-description\">\n      <strong>🎉 Exklusives Geschenk!</strong> Fügen Sie diesen luxuriösen Satin-Kimono kostenlos zu Ihrer Bestellung hinzu. Begrenztes Angebot – nur solange der Vorrat reicht!\n    </div>\n  </div>\n</div>\n\n<script>\n  let giftActive = false;\n  const GIFT_PRODUCT_HANDLE = 'nachtglow-satin-kimono';\n  let giftProductData = null;\n  let originalAddToCart = null;\n\n  // Charger les données du produit cadeau\n  async function loadGiftProduct() {\n    try {\n      const response = await fetch(`/products/${GIFT_PRODUCT_HANDLE}.js`);\n      giftProductData = await response.json();\n      console.log('Produit cadeau chargé:', giftProductData);\n    } catch (error) {\n      console.error('Erreur chargement produit cadeau:', error);\n    }\n  }\n\n  // Trouver le variant ID selon taille ET couleur\n  function findVariantId(size, color) {\n    if (!giftProductData || !giftProductData.variants) return null;\n    \n    const variant = giftProductData.variants.find(v => {\n      return v.option1 === size && v.option2 === color;\n    });\n    \n    console.log('Recherche variant:', size, color, '=> ID:', variant ? variant.id : 'Non trouvé');\n    return variant ? variant.id : null;\n  }\n\n  // Intercepter TOUS les clics sur les boutons d'ajout au panier\n  function interceptAddToCart() {\n    console.log('🔍 Recherche du bouton Add to Cart...');\n    \n    // Attendre que la page soit chargée\n    setTimeout(() => {\n      const buttons = document.querySelectorAll('button[type=\"submit\"], .product-form__submit, [name=\"add\"]');\n      \n      console.log('✅ Boutons trouvés:', buttons.length);\n      \n      buttons.forEach((button, index) => {\n        console.log(`Bouton ${index}:`, button);\n        \n        button.addEventListener('click', async function(e) {\n          console.log('🎯 CLIC DÉTECTÉ sur bouton !');\n          console.log('Toggle actif?', giftActive);\n          \n          if (!giftActive) {\n            console.log('❌ Cadeau pas activé, ajout normal');\n            return; // Laisser continuer normalement\n          }\n          \n          // STOPPER tout\n          e.preventDefault();\n          e.stopImmediatePropagation();\n          e.stopPropagation();\n          \n          console.log('🛑 INTERCEPTION ! Ajout avec cadeau...');\n          \n          const sizeSelect = document.getElementById('giftSizeSelect');\n          const colorSelect = document.getElementById('giftColorSelect');\n          \n          console.log('Taille cadeau:', sizeSelect?.value);\n          console.log('Couleur cadeau:', colorSelect?.value);\n          \n          if (!sizeSelect?.value || !colorSelect?.value) {\n            alert('Bitte wählen Sie Größe und Farbe für Ihr Geschenk');\n            return;\n          }\n          \n          const giftVariantId = findVariantId(sizeSelect.value, colorSelect.value);\n          console.log('Gift Variant ID:', giftVariantId);\n          \n          if (!giftVariantId) {\n            alert('Diese Kombination ist leider nicht verfügbar');\